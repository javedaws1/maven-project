pipeline {
    agent any


    stages {
        stage('SCM Checkout'){
          git 'https://github.com/javedaws1/maven-project'
        }
  }
    {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'JenkinsLocalMaven') {
                    sh 'mvn clean compile'
                }
            }
}
    {
        stage ('test Stage') {

            steps {
                withMaven(maven : 'JenkinsLocalMaven') {
                    sh 'mvn test'
                }
            }
        }
}
    {
        stage ('package Stage') {

            steps {
                withMaven(maven : 'JenkinsLocalMaven') {
                    sh 'mvn package'
                }
            }
          }
}
}
}
